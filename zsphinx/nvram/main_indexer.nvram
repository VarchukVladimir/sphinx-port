###########################################################
# ZRT nvram file, injects tar images contents into ZRT FS #
###########################################################

[fstab] 
channel=/dev/mount/data.tar, mountpoint=/, access=ro, removable=no

[args]
args=single_object_indexer -a

#[debug]
#verbosity=3

[env]
name=SERVER_SOFTWARE,		value=zerocloud

